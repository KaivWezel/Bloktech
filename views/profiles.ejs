<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <% profiles.forEach(profile =>{%>
    <div class="profile">
      <a href="/profile/<%= profile._id %>">
        <p><%= profile.name%></p>
        <p><%= profile.email%></p>
        <p><%= profile._id%></p>
      </a>
    </div>
    <%})%>
  </body>
</html>
